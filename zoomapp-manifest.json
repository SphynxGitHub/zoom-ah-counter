"features": {
  "zoomAppSdk": {
    "enabled": true,
    "apis": [
      "getUserContext",
      "getMeetingContext",
      "getMeetingParticipants",
      "sendAppInvitation",
      "sendAppInvitationToAllParticipants",
      "launchAppInMeeting"
    ]
  }
},
"scopes": [
  {
    "name": "zoomapp:inmeeting",
    "type": "user",
    "required": true,
    "reason": "Used to access meeting context and participants for the Ah-Counter functionality."
  }
],
"allowListDomains": ["zoom-ah-counter.pages.dev"],
"runOn": ["inMeeting"]
